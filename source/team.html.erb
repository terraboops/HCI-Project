---
layout: main
title: Team
---
<h3>Team</h3>
<% 5.times do |i| %>
<div class="team hero-unit">
	<%= image_tag lorem.image('300x300'), :class => 'round pull-left' %>
	<h4><%= lorem.first_name + " " + lorem.last_name %></h4>
	<% 3.times do |j| %>
		<p><%= lorem.paragraph %></p>
	<% end %>
	<div class="clearfix"></div>
</div>
<% end %>